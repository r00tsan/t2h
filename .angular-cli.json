{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "mx-starter"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "app/main.ts",
      "polyfills": "polyfills.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "styles": [
        "styles.scss"
      ],
      "scripts": [],
      "environmentSource": "app/environment.ts",
      "environments": {
        "dev": "app/environment.ts",
        "prod": "app/environment.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./config/protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "tsconfig.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./config/karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {
    }
  }
}
