    "protractor"       : "protractor",
    "protractor:delay" : "sleep 3 && npm run protractor",
    "protractor:live"  : "protractor --elementExplorer",
    "test"             : "./node_modules/.bin/tslint --type-check --project tsconfig.json 'src/**/*.ts' && karma start",
    "tslint"           : "./node_modules/.bin/tslint --type-check --project tsconfig.json 'src/**/*.ts'",
    "e2e"              : "ng e2e",
    "start:dev"        : "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js --config config/webpack.dev.js --progress --profile --watch --hotOnly --content-base src/"
